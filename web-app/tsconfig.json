{
	"compilerOptions": {
		"esModuleInterop": true,
		"isolatedModules": false,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"lib": ["ESNext", "DOM"],
		"module": "ESNext",
		"moduleResolution": "Node",
		"noEmit": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "ESNext",
		"useDefineForClassFields": true
	},
	"include": ["."]
}
