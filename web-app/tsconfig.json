{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@scripts/*": ["src/scripts/*"]
    },
    "isolatedModules": false,
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
